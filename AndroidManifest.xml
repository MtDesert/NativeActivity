<?xml version="1.0"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="games.engines" android:versionCode="1" android:versionName="0.0">
<uses-sdk android:minSdkVersion="9" android:targetSdkVersion="17"/>
	<!-- This is the platform API where NativeActivity was introduced. -->
	<!-- This .apk has no Java code itself, so set hasCode to false. -->
	<application android:label="ApplicationLabel" android:hasCode="false">
		<!-- Our activity is the built-in NativeActivity framework class.This will take care of integrating with our NDK code. -->
		<activity android:name="android.app.NativeActivity" android:label="ActivityLabel" android:configChanges="orientation|keyboardHidden">
			<!-- Tell NativeActivity the name of or .so -->
			<meta-data android:name="android.app.lib_name" android:value="GamesAndroid"/>
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
	</application>
</manifest>